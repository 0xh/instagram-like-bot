{
  "name": "instagram-like-bot",
  "description": "Instagram Like Bot",
  "version": "0.0.1",
  "author": "Husnul Anwari",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:husnulhamidiah/instagram-like-bot.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/husnulhamidiah/instagram-like-bot/issues"
  },
  "keywords": [
    "instagram",
    "instagram-bot",
    "instagram-like"
  ],
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production babel src -d dist",
    "start": "nodemon $2 --ignore cookies --exec babel-node",
    "lint": "eslint --fix src",
    "updateall": "ncu -ua && yarn"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "es2015-node6"
    ],
    "env": {
      "production": {
        "plugins": [
          "add-module-exports"
        ],
        "presets": [
          "es2015",
          "babili"
        ]
      }
    }
  },
  "extends": "standard",
  "plugins": [
    "standard",
    "promise"
  ],
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babili": "^0.1.0",
    "cross-env": "^5.0.0",
    "eslint": "3.17.0",
    "eslint-config-standard": "11.0.0-beta.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "2.2.0",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "cron": "^1.3.0",
    "dotenv": "^5.0.1",
    "instagram-private-api": "^0.6.7",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "ora": "^2.0.0",
    "redis": "^2.8.0"
  }
}
